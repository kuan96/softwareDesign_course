package Homework1;

public class Main {
	
	static void PrintSound(Animal animal)
	{
		animal.sound();
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Dog myDog = new Dog();
		Cat myCat = new Cat();
		Sheep mySheep = new Sheep();
		
		PrintSound(myDog);
		PrintSound(myCat);
		PrintSound(mySheep);
	}

}
